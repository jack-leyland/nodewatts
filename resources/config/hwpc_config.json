{
    "name": "nodewatts_sensor",
    "verbose": false,
    "frequency": 1,
    "output": {
        "type": "mongodb",
        "uri": "mongodb://localhost:27017",
        "database": "nodewatts",
        "collection": "sensor_raw"
    },
    "system": {
        "rapl": {
            "events": [
                "RAPL_ENERGY_PKG"
            ],
            "monitoring_type": "MONITOR_ALL_CPU_PER_SOCKET"
        },
        "msr": {
            "events": [
                "TSC",
                "APERF",
                "MPERF"
            ]
        }
    },
    "container": {
        "core": {
            "events": [
                "CPU_CLK_THREAD_UNHALTED:REF_P",
                "CPU_CLK_THREAD_UNHALTED:THREAD_P",
                "LLC_MISSES",
                "INSTRUCTIONS_RETIRED"
            ]
        }
    }
}